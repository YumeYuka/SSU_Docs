  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vitepress dev --port 25527",
    "build": "vitepress build",
    "preview": "vitepress preview",
    "preview:dist": "vitepress preview --port 4173",
    "clean": "pnpm run clean:cache && pnpm run clean:dist && pnpm run clean:temp",
    "clean:cache": "rimraf .vitepress/.temp .vitepress/cache node_modules/.cache",
    "clean:dist": "rimraf dist",
    "clean:temp": "rimraf .temp .vitepress/.temp",
    "clean:node_modules": "rimraf node_modules pnpm-lock.yaml",
    "clean:all": "pnpm run clean && pnpm run clean:node_modules",
    "deps:check": "pnpm outdated",
    "deps:update": "pnpm update --latest",
    "deps:install": "pnpm install",
    "deps:reinstall": "pnpm run clean:node_modules && pnpm install",
    "build:clean": "pnpm run clean && pnpm run build",
    "dev:debug": "cross-env DEBUG=vitepress:* vitepress dev --port 25527",
    "dev:host": "vitepress dev --host 0.0.0.0 --port 25527",
    "serve": "pnpm run preview",
    "test:build": "pnpm run build && pnpm run preview",
    "analyze": "cross-env ANALYZE=true pnpm run build",
    "check:env": "node scripts/check-env.js",
    "clean:advanced": "node scripts/clean.js",
    "clean:status": "node scripts/clean.js status",
    "health": "pnpm run check:env && pnpm run clean:status"
  },
  "dependencies": {
